{
  "name": "wabe-mongodb-launcher",
  "version": "0.5.2",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "description": "Package to launch the mongodb for test",
  "scripts": {
    "ci": "bun lint",
    "build": "bun --filter wabe-build build:package $(pwd)",
    "lint": "biome lint . --config-path=../../ --no-errors-on-unmatched",
    "format": "biome format --config-path=../../ --write ."
  },
  "dependencies": {
    "mongodb-memory-server": "10.1.4",
    "p-queue": "8.1.0",
    "tcp-port-used": "1.0.2"
  },
  "devDependencies": {
    "@types/tcp-port-used": "1.0.4"
  }
}
