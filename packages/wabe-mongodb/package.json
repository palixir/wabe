{
	"name": "wabe-mongodb",
	"version": "0.5.5",
	"description": "MongoDB adapter for Wabe (official)",
	"keywords": [
		"baas",
		"backend",
		"graphql",
		"mongodb",
		"wabe"
	],
	"homepage": "https://palixir.github.io/wabe/",
	"license": "Apache-2.0",
	"author": {
		"name": "coratgerl",
		"url": "https://github.com/coratgerl"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/palixir/wabe.git"
	},
	"files": [
		"dist"
	],
	"main": "dist/index.js",
	"scripts": {
		"build": "bun --filter wabe-build build:package $(pwd)",
		"check": "tsc --project $(pwd)/tsconfig.json",
		"lint": "oxlint .",
		"ci": "bun check && bun lint $(pwd) && bun test src",
		"format": "oxfmt --write ."
	},
	"dependencies": {
		"mongodb": "6.17.0",
		"p-retry": "7.1.0"
	},
	"devDependencies": {
		"get-port": "7.1.0",
		"uuid": "13.0.0",
		"wabe": "workspace:*",
		"wabe-build": "workspace:*",
		"wabe-mongodb-launcher": "workspace:*"
	}
}
