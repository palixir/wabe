{
	"name": "wabe-resend",
	"version": "0.5.2",
	"description": "Resend email adapter for Wabe (official)",
	"keywords": [
		"baas",
		"backend",
		"graphql",
		"wabe"
	],
	"homepage": "https://palixir.github.io/wabe/",
	"license": "Apache-2.0",
	"author": {
		"name": "coratgerl",
		"url": "https://github.com/coratgerl"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/palixir/wabe.git"
	},
	"main": "dist/index.js",
	"scripts": {
		"build": "bun --filter wabe-build build:package $(pwd)",
		"check": "tsc --project $(pwd)/tsconfig.json",
		"lint": "oxlint .",
		"ci": "bun check && bun lint $(pwd) && bun test src",
		"format": "oxfmt --write ."
	},
	"dependencies": {
		"resend": "4.5.2"
	},
	"devDependencies": {
		"@types/react": "19.0.10",
		"wabe": "workspace:*"
	}
}
