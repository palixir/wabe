{
  "name": "wabe-mistralai",
  "version": "0.5.1",
  "description": "MistralAI adapter for Wabe (official)",
  "homepage": "https://wabe.dev",
  "author": {
    "name": "coratgerl",
    "url": "https://github.com/coratgerl"
  },
  "license": "Apache-2.0",
  "keywords": [
    "backend",
    "wabe",
    "graphql",
    "baas",
    "mistral",
    "ai"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/palixir/wabe.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "bun --filter wabe-build build:package $(pwd)",
    "check": "tsc --project $(pwd)/tsconfig.json",
    "lint": "biome lint . --no-errors-on-unmatched",
    "ci": "bun check && bun lint $(pwd) && bun test src",
    "format": "biome format --write ."
  },
  "dependencies": {
    "@mistralai/mistralai": "1.7.1"
  },
  "devDependencies": {
    "wabe": "workspace:*"
  }
}
